{
  "Kind": "service-router",
  "Name": "business-service",
  "Routes": [
    {
      "Match": {
        "HTTP": {
          "PathPrefix": "/api/v2/"
        }
      },
      "Destination": {
        "Service": "business-service",
        "ServiceSubset": "v2"
      }
    },
    {
      "Match": {
        "HTTP": {
          "PathPrefix": "/api/",
          "Header": [
            {
              "Name": "X-Debug",
              "Present": true
            }
          ]
        }
      },
      "Destination": {
        "Service": "business-service",
        "ServiceSubset": "canary"
      }
    },
    {
      "Match": {
        "HTTP": {
          "PathPrefix": "/api/"
        }
      },
      "Destination": {
        "Service": "business-service",
        "ServiceSubset": "stable"
      }
    }
  ],
  "Meta": {
    "purpose": "traffic-routing",
    "supports": "canary-deployments"
  }
}
